def find_prime_factors(n: int) -> List[int]:
    """Finds the prime factors of a composite number n."""
    prime_factors = []
    # Start with the smallest prime number, 2
    divisor = 2
    while n > 1:
        # If n is divisible by the current divisor, it is a prime factor
        while n % divisor == 0:
            prime_factors.append(divisor)
            n /= divisor
        # Try the next larger integer as the divisor
        divisor += 1
    return prime_factors
